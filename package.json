{
    "name": "resq",
    "version": "0.0.1-alpha.3",
    "description": "REact Selector Query (resq) - Query React components and children by selector (component name)",
    "keywords": [
        "react",
        "selector",
        "resq",
        "rescue",
        "queryselector",
        "query"
    ],
    "author": "Baruch Velez <baruchvelez@gmail.com>",
    "license": "MIT",
    "main": "dist/index.js",
    "umd:main": "dist/index.umd.js",
    "bugs": {
        "url": "https://github.com/baruchvlz/resq/issues"
    },
    "repository": {
        "url": "https://github.com/baruchvlz/resq"
    },
    "dependencies": {
        "fast-deep-equal": "^2.0.1"
    },
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.4.0",
        "@babel/plugin-transform-runtime": "^7.4.0",
        "@babel/preset-env": "^7.4.1",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.5.0",
        "eslint": "^5.15.3",
        "eslint-plugin-jest": "^22.4.1",
        "jest": "^24.5.0",
        "npm-run-all": "^4.1.5",
        "rollup": "^1.7.0",
        "rollup-plugin-babel": "^4.3.2"
    },
    "scripts": {
        "test": "jest ./tests --verbose",
        "lint": "eslint . --fix",
        "build": "node ./scripts/build.js",
        "setup": "run-s transpile rollup",
        "dev": "rollup -c -w",
        "pretranspile": "rimraf babel dist",
        "transpile": "babel . --out-dir babel",
        "rollup": "rollup -c"
    },
    "jest": {
        "transform": {
            "^.+\\.jsx?$": "babel-jest"
        },
        "testPathIgnorePatterns": [
            "node_modules",
            "dist"
        ]
    }
}
